{
    "name": "vitexsoftware/abraflexi-digest",
    "description": "Periodical AbraFlexi digest.",
    "version": "dev-main",
    "authors": [
        {
            "name": "Vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "type": "project",
    "require": {
        "vitexsoftware/ease-bootstrap5-widgets-abraflexi": "* 9999999-dev",
        "vitexsoftware/ease-bootstrap5-widgets": "dev-main",
        "vitexsoftware/abraflexi-bricks": "* dev-main",
        "pear/net_smtp": "dev-master",
        "vitexsoftware/ease-core-bricks": "dev-master",
        "vitexsoftware/ease-html-widgets": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "AbraFlexi\\Digest\\": "src/Digest/",
            "AbraFlexi\\Digest\\Outlook\\": "src/Digest/Outlook/",
            "AbraFlexi\\Digest\\Modules\\": "src/Digest/Modules/",
            "AbraFlexi\\Digest\\Modules\\AllTime\\": "src/Digest/Modules/AllTime",
            "AbraFlexi\\Digest\\Modules\\Monthly\\": "src/Digest/Modules/Monthly"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.61",
        "ergebnis/composer-normalize": "^2.43",
        "ergebnis/php-cs-fixer-config": "^6.34"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
